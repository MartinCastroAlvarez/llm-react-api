{
  "name": "gutenberg-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--no-experimental-global-webcrypto' vite",
    "build:ts": "tsc -p tsconfig.json",
    "build:cdk": "NODE_OPTIONS='--loader ts-node/esm' cdk synth",
    "build:lint": "eslint app.ts src/*.ts app/*.ts --max-warnings 0",
    "build:format": "prettier --write .",
    "build:lambda": "cd app && npm install",
    "build:react": "vite build",
    "build": "yarn build:format && yarn build:lint && yarn build:ts && yarn build:lambda && yarn build:react && yarn build:cdk",
    "deploy": "NODE_OPTIONS='--loader ts-node/esm' cdk deploy --require-approval never",
    "logs": "aws logs tail /aws/lambda/gutenberg-lambda --since 2m --follow",
    "test:web": "curl -X GET https://dl10u8g6h74oc.cloudfront.net",
    "test:api:post": "RESPONSE=$(curl -siX POST https://gmyb98n6d5.execute-api.us-west-2.amazonaws.com/prod/books/1787) && printf '%s\\n' \"$RESPONSE\"",
    "test:api:get": "RESPONSE=$(curl -siX GET https://gmyb98n6d5.execute-api.us-west-2.amazonaws.com/prod/books/1787) && printf '%s\\n' \"$RESPONSE\"",
    "test": "yarn test:api:post && yarn test:api:get && yarn test:web"
  },
  "dependencies": {
    "app-name": "^0.2.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "degit": "^2.8.4",
    "openai": "^4.90.0",
    "react": "18",
    "react-dom": "18",
    "reactflow": "^11.11.4",
    "vite-react-flow-template": "xyflow/vite-react-flow-template"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^22.13.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "10.4.19",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "8.4.35",
    "prettier": "^3.0.0",
    "tailwindcss": "3.4.1",
    "ts-node": "^10.0.0",
    "typescript": "~5.3.3",
    "vite": "^5.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
